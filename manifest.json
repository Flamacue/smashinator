{
  "manifest_version": 2,

  "name": "Smashinator",
  "description": "Replaces all images on a web page with pictures of the band Smash Mouth and Shrek",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["smash.js"]
    }
  ]
}
